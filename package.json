{
  "name": "@launchstack/data",
  "version": "1.0.0-alpha.1",
  "description": "A data modeling tool for TypeScript that combines Zod validation with custom methods, offering minimal boilerplate for structured data handling.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "type-test": "tsd",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:beta": "standard-version --prerelease beta"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launch-stack/data.git"
  },
  "keywords": [
    "data-modeling",
    "typescript",
    "validation",
    "zod",
    "schema",
    "launchstack"
  ],
  "author": "Jvhaile",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/launch-stack/data/issues"
  },
  "homepage": "https://github.com/launch-stack/data#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.13.0",
    "standard-version": "^9.5.0",
    "tsd": "^0.31.2",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
